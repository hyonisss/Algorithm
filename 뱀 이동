#include <stdio.h>
int a[110][110];
int t[110][2];
int time = 0;y
int len = 1;//뱀 길이
int dx[4] = { 0, 1, 0, -1 };
int dy[4] = { 1, 0, -1, 0 };
int N, K, L;
int dir = 0; // 방향 초기값 오른쪽
void main(void)
{
	int i;
	int x, y;
	int d;
	scanf("%d", &N); 
	scanf("%d", &K);
	for (i = 0; i < K; i++)
	{
		scanf("%d %d", &x, &y);
		a[x - 1][y - 1] = 1;
	}
	scanf("%d", &L);
	for (i = 0; i < L; i++)
	{
		scanf("%d %c", &t[i][0], &d);
		if (d == 'D') t[i][1] = 1;
		else t[i][1] 
		a[x - 1][y - 1] = 1;
	}
	time++;

}
