 *******
  *****
   ***
    *
   ***
  *****
 *******
*********

void Print_Star(void)
{

}

void main(void)
{
    Print_Star();
}


2.
#include<stdio.h>
int Sum_Sosu(int n)
{
	int b, sum = 0;
	if (n <= 0) return 0;
	for (int a = 2; a <= n; a++) {
		for (b = 2; b <= a - 1; b++) {
			if (!(a % b)) break;
		}
		if (b <= a - 1) continue;
		else sum += a;

	}
	return sum;
	
}
void main(void)
{
	int n;
	scanf_s("%d", &n);
	printf("%d", Sum_Sosu(n));
}
