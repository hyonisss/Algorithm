#include <stdio.h>
int main() {
    int a;
    int cnt_even = 0;
    int cnt_odd = 0;
    while(1) {
        scanf("%d", &a);
        if(a==0) {
            printf("odd : %d\n", cnt_odd);
            printf("even : %d", cnt_even);
            break;
        }
        else if(a%2==0) cnt_even++;
        else cnt_odd++;

    }
    return 0;
}
